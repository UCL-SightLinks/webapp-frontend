{
  "platform": {
    "apiRuntime": "node:18"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT"
  },
  "buildProperties": {
    "skipBuildDuringDeploy": false,
    "apiBuildCommand": "npm run build",
    "outputLocation": "build"
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
} 